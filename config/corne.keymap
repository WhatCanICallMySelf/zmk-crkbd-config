#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/mouse.h>

/ {
        keymap {
                compatible = "zmk,keymap";



                base {
                        display-name = "Base";
                        bindings = <
   &none &kp Q      &kp W      &kp E       &kp R       &kp T          &kp Y     &kp U       &kp I       &kp O      &kp P        &none
   &none &mt LGUI A &mt LALT S &mt LCTRL D &mt LSHFT F &kp G          &kp H     &mt LSHFT J &mt LCTRL K &mt LALT L &mt LGUI SQT &none
   &none &kp Z      &kp X      &kp C       &kp V       &kp B          &kp N     &kp M       &kp COMMA   &kp DOT    &kp SLASH    &none
                               &lt 4 ESC   &lt 2 SPACE &lt 3 TAB      &lt 6 RET &lt 5 BSPC  &lt 7 DEL
                        >;
                };

                extra {
                        display-name = "Extra";
                        bindings = <
   &none &none &none &none     &none       &none          &none     &none       &none     &none &none &none 
   &none &none &none &none     &none       &none          &none     &none       &none     &none &none &none 
   &none &none &none &none     &none       &none          &none     &none       &none     &none &none &none 
                     &lt 4 ESC &lt 2 SPACE &lt 3 TAB      &lt 6 RET &lt 5 BSPC  &lt 7 DEL
                        >;
                };

                nav {
                        display-name = "Nav";
                        bindings = <
   &studio_unlock &none    &none    &none     &none     &none      &kp K_AGAIN &kp K_PASTE &kp K_COPY &kp K_CUT &kp K_UNDO &none
   &none          &kp LGUI &kp LALT &kp LCTRL &kp LSHFT &none      &caps_word  &kp LEFT    &kp DOWN   &kp UP    &kp RIGHT  &none
   &bootloader    &none    &none    &none     &none     &none      &kp INS     &kp HOME    &kp PG_DN  &kp PG_UP &kp END    &none
                                    &none     &none     &none      &kp RET     &kp BSPC    &kp DEL
                        >;
                };

                mouse {
                        display-name = "Mouse";
                        bindings = <
   &none &none    &none    &none     &none     &none      &kp K_AGAIN &kp K_PASTE    &kp K_COPY     &kp K_CUT    &kp K_UNDO      &none
   &none &kp LGUI &kp LALT &kp LCTRL &kp LSHFT &none      &none       &mmv MOVE_LEFT &mmv MOVE_DOWN &mmv MOVE_UP &mmv MOVE_RIGHT &none
   &none &none    &none    &none     &none     &none      &none       &msc SCRL_LEFT &msc SCRL_DOWN &msc SCRL_UP &msc SCRL_RIGHT &none
                           &none     &none     &none      &mkp RCLK   &mkp LCLK &mkp MCLK
                        >;
                };

                media {
                        display-name = "Media";
                        bindings = <
   &none &none    &none    &none     &none     &none      &none        &none            &none             &none           &none        &none
   &none &kp LGUI &kp LALT &kp LCTRL &kp LSHFT &none      &bt BT_CLR   &kp C_PREVIOUS   &kp C_VOLUME_DOWN &kp C_VOLUME_UP &kp C_NEXT   &none
   &none &none    &none    &none     &none     &none      &bt BT_SEL 0 &bt BT_SEL 1     &bt BT_SEL 2      &bt BT_SEL 3    &bt BT_SEL 4 &none
                           &none     &none     &none      &kp C_STOP   &kp C_PLAY_PAUSE &kp C_MUTE
                        >;
                };

                num {
                        display-name = "Num";
                        bindings = <
   &none &kp LBKT  &kp N7 &kp N8  &kp N9 &kp RBKT       &none &none     &none     &none    &none    &none 
   &none &kp SEMI  &kp N4 &kp N5  &kp N6 &kp EQUAL      &none &kp LSHFT &kp LCTRL &kp LALT &kp LGUI &none 
   &none &kp GRAVE &kp N1 &kp N2  &kp N3 &kp BSLH       &none &none     &none     &none    &none    &none
                          &kp DOT &kp N0 &kp MINUS      &none &none     &none
                        >;
                };

                sym {
                        display-name = "Sym";
                        bindings = <
   &kp  TAB  &kp EXCL &kp AT &kp HASH &kp DLLR &kp PRCNT   &kp CARET &kp AMPS  &kp ASTRK &kp LPAR &kp RPAR &kp BSPC
   &kp LCTRL &trans   &trans &trans   &trans   &trans      &kp MINUS &kp EQUAL &kp LBKT  &kp RBKT &kp BSLH &kp GRAVE
   &kp LSHFT &trans   &trans &trans   &trans   &trans      &kp UNDER &kp PLUS  &kp LBRC  &kp RBRC &kp PIPE &kp TILDE
                             &kp LGUI &trans   &kp SPACE   &kp RET   &trans    &kp RALT
                        >;
                };

                fun {
                        display-name = "Fun";
                        bindings = <
   &kp  TAB  &kp EXCL &kp AT &kp HASH &kp DLLR &kp PRCNT   &kp CARET &kp AMPS  &kp ASTRK &kp LPAR &kp RPAR &kp BSPC
   &kp LCTRL &trans   &trans &trans   &trans   &trans      &kp MINUS &kp EQUAL &kp LBKT  &kp RBKT &kp BSLH &kp GRAVE
   &kp LSHFT &trans   &trans &trans   &trans   &trans      &kp UNDER &kp PLUS  &kp LBRC  &kp RBRC &kp PIPE &kp TILDE
                             &kp LGUI &trans   &kp SPACE   &kp RET   &trans    &kp RALT
                        >;
                };
        };
};
